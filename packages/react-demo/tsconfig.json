{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "jsx": "react",
    "allowJs": true,
    "outDir": "./dist/",
    "target": "es6",
    "sourceMap": true,
    "moduleResolution": "node"
  },
  "include": [
    "./src/**/*"
  ]
}